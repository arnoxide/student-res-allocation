RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^theboxlab\.net$ [OR]
RewriteCond %{HTTP_HOST} ^www\.theboxlab\.net$
RewriteRule ^/?$ "https\:\/\/theboxlab\.net\/" [R=301,L]

